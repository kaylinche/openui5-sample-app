{
	"name": "openui5-sample-app",
	"version": "0.4.0",
	"description": "Sample of an OpenUI5 app",
	"private": false,
	"files": [
		"CONTRIBUTING.md",
		"karma-ci.conf.js",
		"karma.conf.js",
		"LICENSES",
		"LICENSE.txt",
		"ui5.yaml",
		"webapp"
	],
	"engines": {
		"node": ">=18.14.0",
		"npm": ">=9"
	},
	"scripts": {
		"start": "ui5 serve",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "rimraf coverage && karma start karma-ci.conf.js",
		"watch": "npm run karma",
		"test": "npm run lint && npm run karma-ci",
		"build": "ui5 build -a --clean-dest",
		"build-self-contained": "ui5 build self-contained -a --clean-dest",
		"serve-dist": "ws --compress -d dist",
		"deploy2artifactory": "./deploy2Artifactory.sh"
	},
	"devDependencies": {
		"@ui5/cli": "^3.9.1",
		"@ui5/middleware-code-coverage": "^1.1.1",
		"eslint": "^8.57.0",
		"karma": "^6.4.3",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^3.0.4",
		"local-web-server": "^5.3.1",
		"rimraf": "^5.0.5"
	}
}
